{
  "name": "svelte-migration-action",
  "version": "0.0.1",
  "private": true,
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --clean --format cjs",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/svelte-migration-action.git"
  },
  "keywords": [
    "svelte",
    "migration",
    "github-actions",
    "svelte5",
    "static-analysis",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.1",
    "@anthropic-ai/sdk": "^0.52.0",
    "@types/node": "^22.15.29",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "@vitest/coverage-v8": "^3.2.1",
    "eslint": "^9.28.0",
    "eslint-plugin-github": "^6.0.0",
    "glob": "^11.0.2",
    "minimatch": "^10.0.1",
    "prettier": "^3.5.3",
    "tsup": "^8.3.5",
    "typescript": "^5.8.3",
    "vitest": "^3.2.1"
  },
  "packageManager": "pnpm@10.11.1",
  "engines": {
    "pnpm": ">=9.0.0",
    "npm": "forbidden, use pnpm",
    "node": ">=20.0.0"
  }
}
