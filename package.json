{
	"name": "svelte-migration-action",
	"version": "1.0.0",
	"private": true,
	"exports": {
		".": "./dist/index.js"
	},
	"scripts": {
		"build": "tsup src/index.ts --clean --format cjs",
		"package": "npm run build && npm prune --production",
		"test": "vitest",
		"test:coverage": "vitest --coverage",
		"lint": "prettier --check .",
		"format": "prettier --write .",
		"type-check": "tsc --noEmit",
		"changeset": "changeset",
		"version": "changeset version",
		"release": "changeset publish"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/spences10/svelte-migration-action.git"
	},
	"keywords": [
		"svelte",
		"migration",
		"github-actions",
		"svelte5",
		"static-analysis",
		"typescript"
	],
	"author": "Scott Spence <yo@scottspence.com>",
	"license": "MIT",
	"devDependencies": {
		"@actions/core": "^1.11.1",
		"@actions/github": "^6.0.1",
		"@anthropic-ai/sdk": "^0.53.0",
		"@changesets/cli": "^2.29.4",
		"@types/node": "^22.15.29",
		"@typescript-eslint/eslint-plugin": "^8.33.1",
		"@typescript-eslint/parser": "^8.33.1",
		"@vitest/coverage-v8": "^3.2.1",
		"eslint": "^9.28.0",
		"eslint-plugin-github": "^6.0.0",
		"glob": "^11.0.2",
		"minimatch": "^10.0.1",
		"prettier": "^3.5.3",
		"tsup": "^8.5.0",
		"typescript": "^5.8.3",
		"vitest": "^3.2.1"
	},
	"packageManager": "pnpm@10.11.1",
	"engines": {
		"pnpm": ">=9.0.0",
		"npm": "forbidden, use pnpm",
		"node": ">=20.0.0"
	}
}
